{
    "expo": {
        "name": "VacSeen",
        "slug": "VaccineBuddy",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "splash": {
                "image": "./assets/splash.png",
                "resizeMode": "cover"
              },
            "supportsTablet": true
        },
        "android": {
            "package": "mustafasachin.vacseenapp",
            "splash": {
                "mdpi": "./assets/drawable-mdpi/content.png",
                "hdpi": "./assets/drawable-hdpi/content.png",
                "xhdpi": "./assets/drawable-xhdpi/content.png",
                "xxhdpi": "./assets/drawable-xxhdpi/content.png",
                "xxxhdpi": "./assets/drawable-xxxhdpi/content.png",
                "resizeMode": "cover"
              },
            "adaptiveIcon": {
                "foregroundImage": "./assets/256x256.png",
                "backgroundColor": "#79D1D7"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
